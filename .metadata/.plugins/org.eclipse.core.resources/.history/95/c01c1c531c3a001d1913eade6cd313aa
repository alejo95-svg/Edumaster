package com.edumaster.co.interfaceService;

import java.math.BigInteger;
import java.util.List;
import java.util.Optional;

import com.edumaster.co.modelo.Registro;

public interface IRegistroService {
	public List<Registro>listarRegistro();
	public Optional<Registro>listarCedulaRegistro(BigInteger Cedula);
	public int saveRegistro(Registro c);
	public void deleteRegistro(BigInteger Cedula);
}
